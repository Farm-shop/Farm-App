<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="175dp"
    android:layout_height="190dp"
    tools:context=".ProductFragment">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.cardview.widget.CardView
            android:id="@+id/cardView"
            android:layout_width="160dp"
            android:layout_height="180dp"
            android:layout_margin="10dp"
            app:cardBackgroundColor="@color/white"
            app:cardCornerRadius="8dp"
            app:cardElevation="10dp"

            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="160dp"
                android:layout_height="180dp"
                android:background="@color/white">


                <ImageView
                    android:id="@+id/imageView8"
                    android:layout_width="160dp"
                    android:layout_height="95dp"
                    android:scaleType="fitCenter"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.219"
                    app:srcCompat="@drawable/istockphoto" />

                <TextView
                    android:id="@+id/titleProducts"
                    android:layout_width="100dp"
                    android:layout_height="18dp"
                    android:layout_marginTop="12dp"
                    android:text="productName"
                    android:textColor="@color/design_default_color_on_secondary"
                    android:textColorHighlight="@color/black"
                    android:textSize="15dp"
                    android:textStyle="bold"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.144"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imageView8" />

                <TextView
                    android:id="@+id/priceProdect"
                    android:layout_width="80dp"
                    android:layout_height="17dp"
                    android:layout_marginStart="16dp"
                    android:text="0.5 JD/KG"
                    android:textColor="#30e67c"
                    android:textColorHighlight="@color/black"
                    android:textSize="15dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/farmName" />

                <TextView
                    android:id="@+id/farmName"
                    android:layout_width="80dp"
                    android:layout_height="16dp"
                    android:layout_marginStart="16dp"
                    android:text="farmName"
                    android:textColor="@color/browser_actions_title_color"
                    android:textColorHighlight="@color/black"
                    android:textSize="15dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/titleProducts" />

                <ImageButton
                    android:id="@+id/goToitemDetails"
                    android:layout_width="50dp"
                    android:layout_height="40dp"
                    android:layout_marginBottom="16dp"
                    android:background="@drawable/leftround"
                    android:src="@drawable/ic_lock"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent" />


            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.cardview.widget.CardView>
    </androidx.constraintlayout.widget.ConstraintLayout>

